{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "name": "Customer"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "business_type": {
      "model": "business-type"
    },
    "name": {
      "type": "string"
    },
    "phonenumber": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
