{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "name": "Customer",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "business_type": {
      "model": "business-type"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "phonenumber": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "TIN": {
      "type": "string"
    },
    "ContactPerson": {
      "type": "string"
    },
    "KebeleId": {
      "type": "string"
    },
    "password": {
      "type": "password"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Suspended",
        "Terminated",
        "Waiting"
      ]
    },
    "buisness": {
      "type": "string"
    },
    "note": {
      "type": "richtext"
    }
  }
}
